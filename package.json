{
  "name": "hono-minimal",
  "publisher": "RecursiveZero",
  "displayName": "Hono Minimal",
  "type": "module",
  "version": "0.0.1",
  "private": false,
  "description": "A boilerplate of hono with openAPI to reproduce the issue",
  "contributors": [
    {
      "name": "Keshav Mohta"
    }
  ],
  "license": "MIT",
  "funding": {
    "type": "individual",
    "url": "https://xkeshav.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/recursivezero/hono-minimal.git"
  },
  "bugs": {
    "url": "https://github.com/recursivezero/hono-minimal/issues",
    "email": "recursivezero@outlook.com"
  },
  "keywords": [
    "threadzip",
    "server",
    "honojs",
    "rz",
    "recursivezero"
  ],
  "exports": "./dist/index.mjs",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.tsx",
    "start": "cross-env NODE_ENV=development tsx watch src/index.tsx",
    "serve": "cross-env NODE_ENV=production node dist/index.mjs",
    "nuke": "rimraf package-lock.json node_modules local dist coverage",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix"
  },
  "dependencies": {
    "@hono/node-server": "1.19.5",
    "@hono/zod-openapi": "1.1.4",
    "@hono/zod-validator": "0.7.4",
    "@logtape/logtape": "0.6.3",
    "@scalar/hono-api-reference": "0.9.22",
    "@types/bcryptjs": "2.4.6",
    "@types/jsonwebtoken": "9.0.6",
    "axios": "1.12.2",
    "bcryptjs": "2.4.3",
    "dotenv": "16.4.5",
    "dotenv-expand": "11.0.6",
    "hono": "4.10.1",
    "hono-pino": "0.10.3",
    "jsonwebtoken": "9.0.2",
    "mongoose": "8.9.5",
    "multer": "1.4.5-lts.1",
    "openapi-to-md": "1.0.25",
    "pino": "9.14.0",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@types/node": "20.11.25",
    "@typescript-eslint/parser": "8.24.0",
    "cross-env": "7.0.3",
    "eslint": "^9.12.0",
    "eslint-plugin-format": "^0.1.2",
    "pkgroll": "2.12.1",
    "rimraf": "6.0.1",
    "tsc-alias": "1.8.10",
    "tsx": "4.19.3",
    "typescript": "5.6.3"
  },
  "optionalDependencies": {
    "pino-pretty": "11.3.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
